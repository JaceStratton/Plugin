name: JacesPlugin
main: me.jacestratton.JacesPlugin
version: 1.0
commands:
    tip:
        description: Gives you a tip from a list.
        usage: /<command>
    tips:
        description: See how many tips there are.
        usage: /<command>
    addtip:
        description: Add a tip to the list of tips.
        usage: /<command> [tip]